{
  "name": "kiro-extension",
  "version": "0.1.11",
  "description": "Kiro MCP Server - Spec-driven development workflow for Gemini CLI",
  "author": "@kelexine (https://github.com/kelexine)",
  "license": "MIT",
  "type": "module",
  "main": "dist/kiro-server.js",
  "bin": {
    "kiro-extension": "./dist/kiro-server.js"
  },
  "scripts": {
    "build": "esbuild kiro-server.ts --bundle --platform=node --format=esm --outfile=dist/kiro-server.js --banner:js=\"#!/usr/bin/env node\"",
    "watch": "tsc --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "kiro",
    "gemini-cli",
    "spec-driven",
    "development-workflow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kelexine/kiro-extension"
  }
}
